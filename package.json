{
  "name": "nodeblock",
  "version": "0.0.1",
  "description": "A NodeJS DNS server for slow internet connections",
  "main": "index.js",
  "scripts": {
    "start": "ts-node index.ts",
    "dev": "rm *.db; nodemon --watch '**/*.ts' --ignore '**/*.spec.ts' --exec 'ts-node' index.ts"
  },
  "keywords": [
    "nodejs",
    "dns",
    "block",
    "security"
  ],
  "author": "DecentM",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.0.0",
    "eslint-config-nodesecurity": "^1.3.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-security": "^1.3.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "dependencies": {
    "@types/lokijs": "^1.2.30",
    "@types/node": "^8.0.17",
    "chalk-console": "^1.0.1",
    "conf": "^1.1.2",
    "dns-express": "^1.4.1",
    "lokijs": "^1.5.0",
    "multicast-dns": "^6.1.1",
    "nodemon": "^1.11.0",
    "pretty-error": "^2.1.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2"
  }
}
