{
  "name": "nodeblock",
  "version": "0.0.1",
  "description": "A NodeJS DNS server for slow internet connections",
  "main": "index.js",
  "scripts": {
    "start": "babel src -d build && node build/index.js",
    "dev": "rm *.db; nodemon --watch 'src/**/*.js' --exec 'babel-node' src/index.js",
    "build": "babel src -d build"
  },
  "keywords": [
    "nodejs",
    "dns",
    "block",
    "security"
  ],
  "author": "DecentM",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.0.0",
    "eslint-config-nodesecurity": "^1.3.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-security": "^1.3.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "dependencies": {
    "await-to-js": "^1.0.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-3": "^6.24.1",
    "chalk-console": "^1.0.1",
    "conf": "^1.1.2",
    "dns-express": "^1.4.1",
    "eslint-plugin-async": "^0.1.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "flow-bin": "^0.55.0",
    "ip-range-check": "^0.0.2",
    "lokijs": "^1.5.0",
    "moment": "^2.18.1",
    "multicast-dns": "^6.1.1",
    "network": "^0.4.0",
    "nodemon": "^1.11.0",
    "pify": "^3.0.0",
    "pretty-error": "^2.1.1"
  }
}
